{
  "manifest_version": 3,
  "name": "Send Tabs via Email",
  "version": "1.0",
  "description": "A Chrome extension to send open tabs to an email address.",
  "permissions": ["tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}